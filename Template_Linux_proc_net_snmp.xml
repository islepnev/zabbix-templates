    if (!hstr) break;
    var h = hstr.split(': ');
    var h0 = h[0];
    var ha = h[1].split(' ');
    if (isNaN(Number(ha[0]))) {
      headers[h0] = ha;
    } else {
      values[h0] = ha;
    }
}
var data = {};
for (var key in headers) {
    var ha = headers[key];
    var va = values[key];
    var i;
    var subdata = {}
    for (i=0; i&lt;ha.length; i++) {
        var item = ha[i];
        var value = va[i];
        subdata[item] = value;
    }
    data[key] = subdata;
}
return JSON.stringify(data, null, 2);
